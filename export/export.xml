<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
	<script file="export.lua"/>
	<Frame name="exportFrame" parent="UIParent" hidden="true">
		<Scripts>
			<OnLoad>
				export_OnLoad()
			</OnLoad>
			<OnEvent>
				export_OnEvent()
			</OnEvent>
		</Scripts>
	</Frame>
	<Frame name="export_ExportFrame" frameStrata="DIALOG" toplevel="true" parent="UIParent" movable="false" enableMouse="true" hidden="true" clampedToScreen="true">
		<Size>
			<AbsDimension x="500" y="700"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
		</Layers>
		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\Tooltips\UI-Tooltip-Border">			<BackgroundInsets>
				<AbsInset bottom="4" left="4" right="4" top="4"></AbsInset>
			</BackgroundInsets>
			<EdgeSize>
				<AbsValue val="16"></AbsValue>
			</EdgeSize>
		</Backdrop>
		<Frames>
			<Button inherits="UIPanelCloseButton" name="$parent_CloseButton">
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="0" y="0"></AbsDimension>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>export_ExportFrame:Hide();</OnClick>
				</Scripts>
			</Button>
			<EditBox name="$parentEditBox1" inherits="InputBoxTemplate" multiLine="true" autofocus="false" >
				<Size>
					<AbsDimension x="480" y="680"/>
				</Size>
				<Anchors>
